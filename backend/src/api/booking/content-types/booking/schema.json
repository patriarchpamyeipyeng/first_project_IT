{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "Booking"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service",
      "mappedBy": "booking"
    },
    "email": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "datetime",
      "required": true
    },
    "stati": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        "confirmed",
        "cancelled"
      ]
    },
    "vehicleType": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "notes": {
      "type": "blocks"
    }
  }
}
