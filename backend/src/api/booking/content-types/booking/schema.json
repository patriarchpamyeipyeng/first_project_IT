{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "Booking"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "service": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::service.service"
    },
    "email": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "datetime",
      "required": true
    },
    "stati": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        "confirmed",
        "cancelled"
      ]
    }
  }
}
